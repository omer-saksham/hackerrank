#include <stdio.h>
int gcd(int a, int b) {
    return b ? gcd(b, a % b) : a;
}

int main() {
    int T;
    scanf("%d", &T); 
    while (T--) {
        int a, b, c;
        scanf("%d %d %d", &a, &b, &c);
        if (c <= (a > b ? a : b) && c % gcd(a, b) == 0)
            printf("YES\n");
        else
            printf("NO\n");
    }

    return 0;
}
