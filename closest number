#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

int compare(const void* a, const void* b) {
    return (*(int*)a - *(int*)b);
}

int main() {
    int n;
    scanf("%d", &n);

    int* arr = malloc(n * sizeof(int));
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    qsort(arr, n, sizeof(int), compare);

    int minDiff = INT_MAX;
    for (int i = 1; i < n; i++) {
        int diff = arr[i] - arr[i - 1];
        if (diff < minDiff) {
            minDiff = diff;
        }
    }

    for (int i = 1; i < n; i++) {
        int diff = arr[i] - arr[i - 1];
        if (diff == minDiff) {
            printf("%d %d ", arr[i - 1], arr[i]);
        }
    }

    free(arr);
    return 0;
}
