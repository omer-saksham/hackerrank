#include <stdio.h>

int max(int a, int b) {
    return (a > b) ? a : b;
}

int main() {
    int q;
    scanf("%d", &q);

    while (q--) {
        int n;
        scanf("%d", &n);

        int size = 2 * n;
        int matrix[size][size];

        for (int i = 0; i < size; i++) {
            for (int j = 0; j < size; j++) {
                scanf("%d", &matrix[i][j]);
            }
        }

        int total = 0;

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                int a = matrix[i][j];
                int b = matrix[i][size - 1 - j];
                int c = matrix[size - 1 - i][j];
                int d = matrix[size - 1 - i][size - 1 - j];

                int best = a;
                if (b > best) best = b;
                if (c > best) best = c;
                if (d > best) best = d;

                total += best;
            }
        }

        printf("%d\n", total);
    }

    return 0;
}
