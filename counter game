#include <stdio.h>

int isPowerOfTwo(unsigned long long n) {
    return (n & (n - 1)) == 0;
}

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        unsigned long long n;
        scanf("%llu", &n);

        int moves = 0;

        while (n != 1) {
            if (isPowerOfTwo(n)) {
                n /= 2;
            } else {
                unsigned long long p = 1ULL << 63; 
                while (p > n) {
                    p >>= 1;
                }
                n -= p;
            }
            moves++;
        }

        if (moves % 2 == 1) {
            printf("Louise\n");
        } else {
            printf("Richard\n");
        }
    }

    return 0;
}
