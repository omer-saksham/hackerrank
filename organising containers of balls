#include <stdio.h>
#include <stdlib.h>

int compare(const void *a, const void *b) {
    long long x = *(const long long *)a;
    long long y = *(const long long *)b;
    if (x < y) return -1;
    else if (x > y) return 1;
    else return 0;
}

int main() {
    int q;  // Number of queries/test cases
    scanf("%d", &q);

    while (q--) {
        int n;
        scanf("%d", &n);

        long long container_sum[n];
        long long type_sum[n];

        for (int i = 0; i < n; i++) {
            container_sum[i] = 0;
            type_sum[i] = 0;
        }

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                long long val;
                scanf("%lld", &val);
                container_sum[i] += val;
                type_sum[j] += val;
            }
        }

        qsort(container_sum, n, sizeof(long long), compare);
        qsort(type_sum, n, sizeof(long long), compare);

        int possible = 1;
        for (int i = 0; i < n; i++) {
            if (container_sum[i] != type_sum[i]) {
                possible = 0;
                break;
            }
        }

        if (possible) {
            printf("Possible\n");
        } else {
            printf("Impossible\n");
        }
    }

    return 0;
}
