#include <stdio.h>
#include <string.h>

long long substrings(char* n) {
    long long MOD = 1000000007;
    int len = strlen(n);
    if (len == 0) return 0;

    long long current_dp_sum = 0;
    long long total_sum = 0;

    for (int i = 0; i < len; i++) {
        long long digit = n[i] - '0';
        
        long long term1 = (current_dp_sum * 10) % MOD;
        
        long long term2 = ((long long)(i + 1) * digit) % MOD;
        
        current_dp_sum = (term1 + term2) % MOD;

        total_sum = (total_sum + current_dp_sum) % MOD;
    }

    return total_sum;
}

int main() {
    char n[200005];
    if (scanf("%s", n) == 1) {
        printf("%lld\n", substrings(n));
    }
    return 0;
}
