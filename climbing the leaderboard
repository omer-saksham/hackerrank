#include <stdio.h>
#include <stdlib.h>

int removeDuplicates(int *arr, int n) {
    int j = 0;
    for (int i = 1; i < n; i++) {
        if (arr[i] != arr[j]) {
            j++;
            arr[j] = arr[i];
        }
    }
    return j + 1;
}

int main() {
    int n, m;
    scanf("%d", &n);
    int scores[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &scores[i]);
    }

    scanf("%d", &m);
    int alice[m];
    for (int i = 0; i < m; i++) {
        scanf("%d", &alice[i]);
    }

    int uniqueScores[n];
    for (int i = 0; i < n; i++) {
        uniqueScores[i] = scores[i];
    }

    int size = removeDuplicates(uniqueScores, n);

    int i = size - 1;
    for (int j = 0; j < m; j++) {
        while (i >= 0 && alice[j] >= uniqueScores[i]) {
            i--;
        }
        printf("%d\n", i + 2);
    }

    return 0;
}
